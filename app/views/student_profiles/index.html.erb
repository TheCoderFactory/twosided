<h1>Listing student_profiles</h1>

<table class="table">
    <tr>
      <th>First_name</th>
      <th>Last_name</th>
      <th>Address_line_1</th>
      <th>Address_line_2</th>
      <th>Suburb</th>
      <th>Postcode</th>
      <th>State</th>
      <th>Website_url</th>
      <th>Contact_number</th>
      <th>Degree</th>
      <th>Institution</th>
      <th>Working_with_children_number</th>
      <th>Profile_pic</th>
      <th>Induction</th>
      <th>Bio</th>
        <th colspan="3"></th>
    </tr>

  <%= content_tag_for(:tr, @student_profiles) do |student_profile| %>
    <td><%= student_profile.first_name %></td>
    <td><%= student_profile.last_name %></td>
    <td><%= student_profile.address_line_1 %></td>
    <td><%= student_profile.address_line_2 %></td>
    <td><%= student_profile.suburb %></td>
    <td><%= student_profile.postcode %></td>
    <td><%= student_profile.state %></td>
    <td><%= student_profile.website_url %></td>
    <td><%= student_profile.contact_number %></td>
    <td><%= student_profile.degree %></td>
    <td><%= student_profile.institution %></td>
    <td><%= student_profile.working_with_children_number %></td>
    <td><%= student_profile.profile_pic %></td>
    <td><%= student_profile.induction %></td>
    <td><%= student_profile.bio %></td>
      <td><%= link_to 'Show', student_profile %></td>
    <td><%= link_to 'Edit', edit_student_profile_path(student_profile), class: 'btn btn-edit' %></td>
    <td><%= link_to 'Delete', student_profile, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-destroy' %></td>
  <% end %>
</table>


<div class="form-actions">
  <%= link_to 'New Student profile', new_student_profile_path, class: 'btn btn-success' %>
</div>
