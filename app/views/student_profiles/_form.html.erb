<%= simple_form_for(@student_profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name %>
    <%= f.input :last_name %>
    <%= f.input :address_line_1 %>
    <%= f.input :address_line_2 %>
    <%= f.input :suburb %>
    <%= f.input :postcode %>
    <%= f.input :state %>
    <%= f.input :website_url %>
    <%= f.input :contact_number %>
    <%= f.input :degree %>
    <%= f.input :institution %>
    <%= f.input :working_with_children_number %>
    <%= f.input :profile_pic %>
    <%= f.input :induction %>
    <%= f.input :bio %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
